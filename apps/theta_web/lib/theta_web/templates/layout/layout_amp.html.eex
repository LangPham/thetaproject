<!doctype html><html amp lang="vi"><head>	<meta charset="utf-8">	<meta http-equiv="X-UA-Compatible" content="IE=edge"/>	<meta name="viewport" content="width=device-width, initial-scale=1.0"/>	<script async src="https://cdn.ampproject.org/v0.js"></script>	<script async custom-element="amp-form" src="https://cdn.ampproject.org/v0/amp-form-0.1.js"></script>	<script	async	custom-element="amp-sidebar" src="https://cdn.ampproject.org/v0/amp-sidebar-0.1.js"	></script>	<script async custom-element="amp-position-observer" src="https://cdn.ampproject.org/v0/amp-position-observer-0.1.js"></script>	<script async custom-element="amp-animation" src="https://cdn.ampproject.org/v0/amp-animation-0.1.js"></script>	<script async custom-element="amp-youtube" src="https://cdn.ampproject.org/v0/amp-youtube-0.1.js"></script>	<style amp-boilerplate>      body {          -webkit-animation: -amp-start 8s steps(1, end) 0s 1 normal both;          -moz-animation: -amp-start 8s steps(1, end) 0s 1 normal both;          -ms-animation: -amp-start 8s steps(1, end) 0s 1 normal both;          animation: -amp-start 8s steps(1, end) 0s 1 normal both      }      @-webkit-keyframes -amp-start {          from {              visibility: hidden          }          to {              visibility: visible          }      }      @-moz-keyframes -amp-start {          from {              visibility: hidden          }          to {              visibility: visible          }      }      @-ms-keyframes -amp-start {          from {              visibility: hidden          }          to {              visibility: visible          }      }      @-o-keyframes -amp-start {          from {              visibility: hidden          }          to {              visibility: visible          }      }      @keyframes -amp-start {          from {              visibility: hidden          }          to {              visibility: visible          }      }	</style>	<noscript>		<style amp-boilerplate>        body {            -webkit-animation: none;            -moz-animation: none;            -ms-animation: none;            animation: none        }		</style>	</noscript>	<!--<link rel="preload" href="https://theta.vn/images/logo.svg" as="image" type="image/svg+xml">	<link rel="preload" href="/fonts/tt-ico.ttf" as="font" type="font/ttf" crossorigin>-->	<!--<link rel="preload" href='<%= Routes.static_path(@conn, "/js/front.js") %>' as="script">-->	<%  {ok, css } = File.read(Path.join(:code.priv_dir(:theta_web), "static/css/front.css")) %>	<style amp-custom>		<%= raw css %>	</style>	<link rel="preconnect" href="https://www.googletagmanager.com" crossorigin>	<link rel="preconnect" href="https://www.google-analytics.com" crossorigin>	<!--<script defer src="https://www.googletagmanager.com/gtag/js?id=UA-20920387-11"></script>	<script defer src='<%= Routes.static_path(@conn, "/js/script.js") %>'></script>-->	<title><%= @page.head.title %> | Theta.vn</title>	<meta property="og:title" content="<%= @page.head.title %>">	<meta name="description" content="<%= @page.head.description %>">	<meta property="og:description" content="<%= @page.head.description %>">	<link rel="shortcut icon" href="/favicon.ico">	<link rel="icon" type="image/svg+xml" href="/images/logo_favicon.svg">	<link rel="apple-touch-icon" href="/images/logo_favicon_192.png">	<meta property="og:url" content="<%= @page.head.canonical %>"/>	<%= render(ThetaWeb.HeadView, "head.html", head: @page.head) %></head><body class="webp"><header class="layout" id="top">	<amp-position-observer on="enter:hideAnim.start; exit:showAnim.start"	                       layout="nodisplay">	</amp-position-observer>	<section class="container">		<nav class="navbar" role="navigation" aria-label="main navigation">			<div class="navbar-brand">				<a href="/" class="navbar-item-brand">						<amp-img										alt="A view of the sea"										src="/images/logo.svg"										width="160"										height="60"						>						</amp-img>				</a>			</div>			<div id="navbarMenu" class="navbar-menu">				<div role="button" on="tap:sidebar-menu.toggle" tabindex="0" class="hamburger" aria-label="Menu">					<span></span>					<span></span>					<span></span>					<span></span>				</div>			</div>		</nav>	</section></header><amp-sidebar id="sidebar-menu" layout="nodisplay" side="right" class="menu-layer">	<div class="close">		<div  class="hamburger is-active"		      role="button"		      aria-label="close sidebar"		      on="tap:sidebar-menu.toggle"		      tabindex="0"		>			<span></span>			<span></span>			<span></span>			<span></span>		</div>	</div>	<%= render(ThetaWeb.MenuView, "menu.html", assigns ) %></amp-sidebar><main>	<%= @inner_content %></main><amp-animation id="showAnim"               layout="nodisplay">	<script type="application/json">		{			"duration": "200ms",			"fill": "both",			"iterations": "1",			"direction": "alternate",			"animations": [				{					"selector": "#topBtn",					"keyframes": [						{ "opacity": "1", "visibility": "visible" }					]				}			]		}	</script></amp-animation><amp-animation id="hideAnim"               layout="nodisplay">	<script type="application/json">		{			"duration": "200ms",			"fill": "both",			"iterations": "1",			"direction": "alternate",			"animations": [				{					"selector": "#topBtn",					"keyframes": [						{ "opacity": "0", "visibility": "hidden" }					]				}			]		}	</script></amp-animation><button id="topBtn" on="tap:top.scrollTo(duration=200)" title="Go to top"><i class="tti ic-up"></i></button><%= render(ThetaWeb.AmpView, "footer.html", assigns ) %></body></html>